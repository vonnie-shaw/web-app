pipeline{
	agent any 
	tools{
		maven "Maven3.8.4"
	}
	stages{
		stage("1. Initiation"){
			steps{
				sh "echo start of application"
			}
		}
		stage("2. git clone"){
			steps{
				git "https://github.com/vonnie-shaw/web-app.git"
			}
		}
		stage("3. Build"){
			steps{
				sh "mvn package"
			}
		}
		}
